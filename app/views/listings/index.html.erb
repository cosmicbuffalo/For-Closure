     $('#map-search-button').on("click", function(e){
    e.preventDefault()
    console.log($("#search-bar-input").serialize())
    
    $.post({
        url: '/listings/partial_search',
        data: $("#search-bar-input").serialize(),
        dataType: 'json',
        success: function(res){
          console.log(res)
          map.setCenter(res[0],res[1])

        }

      })


  })%>
    
    
